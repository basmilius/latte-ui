{
	"name": "@bybas/latte-ui",
	"version": "1.9.0-beta.22",
	"description": "An easy to use UI library.",
	"author": {
		"name": "Bas Milius",
		"email": "bas@mili.us",
		"url": "https://bas.dev"
	},
	"repository": "https://github.com/basmilius/latte-ui",
	"private": true,
	"scripts": {
		"build": "yarn workspace @bybas/latte-ui run build",
		"build:all": "yarn run build && yarn run docs:build",
		"serve": "yarn workspace @bybas/latte-ui run serve",
		"docs:build": "yarn workspace @bybas/latte-ui-docs run build",
		"docs:serve": "yarn workspace @bybas/latte-ui-docs run serve",
		"publish": "yarn workspace @bybas/latte-ui publish --access public",
		"publish:beta": "yarn workspace @bybas/latte-ui publish --access public --tag beta",
		"workspace:install": "yarn install"
	},
	"workspaces": [
		"packages/*"
	],
	"postcss": {
		"plugins": {
			"autoprefixer": {}
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not ie <= 8"
	]
}
